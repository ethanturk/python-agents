# OpenAI / Local LLM Configuration
OPENAI_API_BASE=http://192.168.5.203:1234/v1
OPENAI_API_KEY=lm-studio
OPENAI_MODEL=gpt-oss-20b
OPENAI_EMBEDDING_MODEL=text-embedding-nomic-embed-text-v1.5
OPENAI_EMBEDDING_DIMENSIONS=768

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key-here
VECTOR_TABLE_NAME=documents

# Azure Storage Configuration
AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=...;AccountKey=...;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONTAINER_NAME=documents
AZURE_STORAGE_ACCOUNT_NAME=yourstorageaccount

# Queue Service Configuration
QUEUE_PROVIDER=mock
QUEUE_SERVICE_URL=http://localhost:8000

# Firebase Configuration
FIREBASE_REQUIRED=false
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# SQLite Database Configuration
SQLITE_DB_PATH=/tmp/summaries.db

# Deployment Configuration
RUN_WORKER_EMBEDDED=false

# Logging
LOG_LEVEL=info
NODE_ENV=development
