{
  "version": 2,
  "buildCommand": "pnpm run build",
  "builds": [
    {
      "src": "agent/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "documents/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "summaries/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "notifications/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/agent/summaries",
      "dest": "/summaries/index.ts"
    },
    {
      "src": "/api/agent/summary_qa",
      "dest": "/summaries/index.ts"
    },
    {
      "src": "/api/agent/search_qa",
      "dest": "/summaries/index.ts"
    },
    {
      "src": "/api/agent/summarize",
      "dest": "/summaries/index.ts"
    },
    {
      "src": "/api/agent/documents",
      "dest": "/documents/index.ts"
    },
    {
      "src": "/api/agent/documentsets",
      "dest": "/documents/index.ts"
    },
    {
      "src": "/api/agent/upload",
      "dest": "/documents/index.ts"
    },
    {
      "src": "/api/agent/files/(.*)",
      "dest": "/documents/index.ts"
    },
    {
      "src": "/api/agent/(.*)",
      "dest": "/agent/index.ts"
    },
    {
      "src": "/api/documents/(.*)?",
      "dest": "/documents/index.ts"
    },
    {
      "src": "/api/summaries/(.*)?",
      "dest": "/summaries/index.ts"
    },
    {
      "src": "/api/poll",
      "dest": "/notifications/index.ts"
    },
    {
      "src": "/api/notifications/(.*)?",
      "dest": "/notifications/index.ts"
    },
    {
      "src": "/health",
      "dest": "/agent/index.ts"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
        }
      ]
    }
  ]
}
