# Worker Environment Configuration
# Copy this file to .env and fill in the required values

# Client Configuration
# Unique identifier for this client instance (e.g., southhaven, demo, production)
# Used to create isolated queue: {CLIENT_ID}-tasks
CLIENT_ID=southhaven

# Worker Configuration
# How often (in seconds) to poll the queue for new messages
WORKER_POLLING_INTERVAL=5

# How long (in seconds) a message is hidden from other workers while being processed
# Increase this if tasks take longer than 30 seconds
WORKER_VISIBILITY_TIMEOUT=30

# Maximum number of messages to fetch per poll
WORKER_MAX_MESSAGES=10

# Azure Storage Configuration
# Connection string for Azure Storage account (queues and blobs)
# Format: DefaultEndpointsProtocol=https;AccountName=<account>;AccountKey=<key>;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONNECTION_STRING=

# Azure Storage container name for file storage
AZURE_STORAGE_CONTAINER_NAME=documents

# Azure Storage account name
AZURE_STORAGE_ACCOUNT_NAME=

# OpenAI / LLM Configuration
# OpenAI API key or local LLM key
OPENAI_API_KEY=sk-...

# OpenAI API base URL (for local models like LM Studio)
# Default: http://192.168.5.203:1234/v1
OPENAI_API_BASE=https://api.openai.com/v1

# OpenAI model name
OPENAI_MODEL=gpt-4

# OpenAI embedding model
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Embedding dimensions (for vector DB)
OPENAI_EMBEDDING_DIMENSIONS=1536

# Supabase Configuration
# Supabase project URL
SUPABASE_URL=https://xxx.supabase.co

# Supabase service role key (for backend access)
SUPABASE_KEY=eyJ...

# Optional: Queue Provider (mock | azure)
# Set to 'mock' for local testing without Azure Queue
QUEUE_PROVIDER=azure
