{
  "name": "python-agents-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Node.js serverless backend for python-agents",
  "type": "module",
  "scripts": {
    "dev": "vercel dev",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"backend-nodejs/**/*.ts\" \"api/*/index.ts\"",
    "lint:fix": "eslint \"backend-nodejs/**/*.ts\" \"api/*/index.ts\" --fix",
    "format": "prettier --write \"backend-nodejs/**/*.ts\" \"api/**/*.ts\"",
    "format:check": "prettier --check \"backend-nodejs/**/*.ts\" \"api/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "workspaces": [
    "api/agent",
    "api/documents",
    "api/summaries",
    "api/notifications"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vercel": "^50.1.6",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "@fastify/cors": "^8.4.0",
    "@supabase/supabase-js": "^2.38.0",
    "fastify": "^4.24.0",
    "firebase-admin": "^12.0.0",
    "openai": "^4.20.0",
    "pino": "^8.17.0",
    "pino-pretty": "^10.2.0",
    "sql.js": "^1.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
