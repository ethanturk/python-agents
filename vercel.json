{
  "$schema": "https://vercel.com/schemas/v1/config.json",
  "buildCommand": "cd frontend && VITE_BASE_PATH=/southhaven npm run build",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd frontend && npm install",
  "outputDirectory": "frontend/dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)\\.js$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.css$",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/southhaven/api/:path*",
      "destination": "/backend/api/index.py"
    },
    {
      "source": "/southhaven/agent/:path*",
      "destination": "/backend/api/index.py"
    },
    {
      "source": "/southhaven/sse",
      "destination": "/backend/api/index.py"
    },
    {
      "source": "/southhaven/ws",
      "destination": "/backend/api/index.py"
    },
    {
      "source": "/southhaven/internal/:path*",
      "destination": "/backend/api/index.py"
    },
    {
      "source": "/southhaven/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/southhaven/icon.svg",
      "destination": "/icon.svg"
    },
    {
      "source": "/southhaven/vite.svg",
      "destination": "/vite.svg"
    },
    {
      "source": "/southhaven/:path*",
      "destination": "/index.html"
    }
  ]
}
