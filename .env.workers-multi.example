# Multi-Instance Worker Environment Variables
# Copy this file to .env on your VPS at /root/workers/.env
# Update with actual values for each tenant

# Flower Monitoring (Optional - defaults to admin:admin)
FLOWER_BASIC_AUTH=admin:changeme

# ============================================
# TENANT 1 Configuration
# ============================================
TENANT1_CELERY_BROKER_URL=redis://default:password@us1-tenant1.upstash.io:12345  # pragma: allowlist secret
TENANT1_OPENAI_API_KEY=sk-proj-xxx
TENANT1_OPENAI_API_BASE=https://api.openai.com/v1
TENANT1_OPENAI_MODEL=gpt-4o-mini
TENANT1_OPENAI_EMBEDDING_MODEL=text-embedding-3-small
TENANT1_OPENAI_EMBEDDING_DIMENSIONS=1536
TENANT1_SUPABASE_URL=https://xxx.supabase.co
TENANT1_SUPABASE_KEY=eyJhbGci...
TENANT1_AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=tenant1storage;AccountKey=xxx;EndpointSuffix=core.windows.net
TENANT1_AZURE_STORAGE_ACCOUNT_NAME=tenant1storage
TENANT1_AZURE_STORAGE_CONTAINER_NAME=documents
TENANT1_VECTOR_TABLE_NAME=documents

